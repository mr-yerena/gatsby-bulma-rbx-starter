{
  "plugins": [
    "import-quotes"
  ],
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [ ".js", ".jsx" ] } ],
    "quotes": ["error", "backtick"],
    "semi": ["error", "never"],
    "import-quotes/import-quotes": [1, "single"],
    "react/jsx-curly-brace-presence": "off"
  },
  "globals": {
    "fetch": false
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}